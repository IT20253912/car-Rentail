<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         
         
 <servlet>
 	<servlet-name>login</servlet-name>
 	<servlet-class>com.paymentcus.Loginservlet</servlet-class>
 </servlet>
 	
<servlet-mapping>
     <servlet-name>login</servlet-name>
     <url-pattern>/log</url-pattern>
</servlet-mapping>



<servlet>
	<servlet-name>customer</servlet-name>
	<servlet-class>com.paymentcus.Customerinsert</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>customer</servlet-name>
	<url-pattern>/insert</url-pattern>
</servlet-mapping>


<servlet>
	<servlet-name>cusDetails</servlet-name>
	<servlet-class>com.paymentcus.cusdetails</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>cusDetails</servlet-name>
	<url-pattern>/insertcus</url-pattern>
</servlet-mapping>



<servlet>
	<servlet-name>cusPayment</servlet-name>
	<servlet-class>com.paymentcus.cusPayment</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>cusPayment</servlet-name>
	<url-pattern>/insertcard</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>search</servlet-name>
	<servlet-class>com.paymentcus.SearchServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>search</servlet-name>
	<url-pattern>/search</url-pattern>
</servlet-mapping>


         
</web-app>